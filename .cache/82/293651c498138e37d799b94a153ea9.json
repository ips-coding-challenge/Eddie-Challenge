ont-size: 10px;\r\n}\r\n\r\nh1,\r\nh2,\r\nh3,\r\nh4,\r\nh5,\r\nh6,\r\na,\r\np,\r\ninput {\r\n  font-family: \"Poppins\", sans-serif;\r\n}\r\n\r\np {\r\n  font-size: 1.2rem;\r\n  line-height: 1.8rem;\r\n  color: var(--clr-gray2);\r\n}\r\n\r\n.container {\r\n  max-width: 1110px;\r\n  padding: 0 40px;\r\n  margin: 0 auto;\r\n}\r\n\r\n.container-lg {\r\n  padding: 0 10px !important;\r\n}\r\n// .container-sm {\r\n//   max-width: 320px;\r\n//   margin: initial;\r\n// }\r\n\r\nh2 {\r\n  font-size: 24px;\r\n  line-height: 36px;\r\n  font-weight: 500;\r\n  color: var(--clr-gray1);\r\n}\r\n\r\nimg {\r\n  width: 100%;\r\n  max-width: 100%;\r\n  height: auto;\r\n  border-radius: 24px;\r\n}\r\n\r\n.btn {\r\n  padding: 10px 20px;\r\n  border-radius: 12px;\r\n  border: 0;\r\n  font-weight: 500;\r\n  font-size: 16px;\r\n  line-height: 24px;\r\n}\r\n\r\n.btn-default {\r\n  background-color: #e0e0e0;\r\n  color: #828282;\r\n}\r\n.btn-blue,\r\n.btn-green,\r\n.btn-red {\r\n  color: white;\r\n}\r\n.btn-blue {\r\n  background: var(--clr-blue);\r\n}\r\n.btn-green {\r\n  background: var(--clr-green);\r\n}\r\n.btn-red {\r\n  background: var(--clr-red);\r\n}\r\n\r\nsection {\r\n  margin-bottom: 100px;\r\n}\r\n\r\n.form {\r\n  margin-top: 31px;\r\n}\r\n.form label {\r\n  font-size: 10px;\r\n  line-height: 15px;\r\n  color: var(--clr-gray3);\r\n}\r\n.btn-input {\r\n  background-color: #f2f2f2;\r\n  height: 47px;\r\n  border-radius: 12px;\r\n  padding: 4px 4px 4px 19px;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  margin-top: 4px;\r\n}\r\ninput {\r\n  outline: none;\r\n  background-color: transparent;\r\n  border: none;\r\n  max-width: 60%;\r\n}\r\ninput::placeholder {\r\n  font-size: 14px;\r\n  line-height: 21px;\r\n  color: var(--clr-gray4);\r\n}\r\n\r\n// Home\r\n.home {\r\n  margin-top: 48px;\r\n\r\n  .question {\r\n    font-size: 1.2rem;\r\n    line-height: 1.8rem;\r\n    font-weight: 500;\r\n    color: var(--clr-blue);\r\n  }\r\n\r\n  .home__image {\r\n    margin-bottom: 22px;\r\n    margin-top: 15px;\r\n    border-radius: 18px;\r\n  }\r\n\r\n  p {\r\n    margin-top: 33px;\r\n  }\r\n\r\n  .form {\r\n    margin-top: 31px;\r\n  }\r\n}\r\n\r\n// Services\r\n.feature {\r\n  margin-top: 94px;\r\n  padding: 46px 35px;\r\n\r\n  h3 {\r\n    margin-top: 3.5rem;\r\n    font-weight: bold;\r\n    font-size: 2.4rem;\r\n    line-height: 3.6rem;\r\n    color: var(--clr-gray1);\r\n  }\r\n  p {\r\n    margin-top: 2.4rem;\r\n    font-size: 1.6rem;\r\n    line-height: 2.4rem;\r\n    color: var(--clr-gray2);\r\n  }\r\n  .btn {\r\n    margin-top: 30px;\r\n  }\r\n\r\n  &.rounded {\r\n    background: #ffffff;\r\n    box-shadow: 0px 10px 30px rgba(51, 51, 51, 0.1);\r\n    border-radius: 24px;\r\n  }\r\n}\r\n\r\n.feat-icon {\r\n  height: 67px;\r\n  width: 67px;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n// Our Works\r\n.works {\r\n  display: initial;\r\n}\r\n.work {\r\n  margin-top: 4.8rem;\r\n\r\n  &__image {\r\n    margin-bottom: 1.2rem;\r\n  }\r\n  &__type {\r\n    font-weight: 300;\r\n    font-size: 1.2rem;\r\n    line-height: 1.8rem;\r\n    color: var(--clr-gray3);\r\n  }\r\n\r\n  &__title {\r\n    font-size: 1.8rem;\r\n    line-height: 2.7rem;\r\n    color: var(--clr-gray1);\r\n  }\r\n}\r\n\r\n.link-special {\r\n  text-decoration: none;\r\n  color: var(--clr-blue);\r\n  font-size: 18px;\r\n  line-height: 27px;\r\n  display: flex;\r\n  align-items: center;\r\n  margin-top: 3.5rem;\r\n}\r\n.link-special span {\r\n  margin-right: 12px;\r\n}\r\n\r\n// Media Queries\r\n@media screen and (min-width: 767px) {\r\n  .container-lg {\r\n    padding: 0 100px !important;\r\n  }\r\n  .container-sm {\r\n    max-width: 50%;\r\n  }\r\n  .features-list {\r\n    display: flex;\r\n  }\r\n}\r\n\r\n@media screen and(min-width: 860px) {\r\n  .container {\r\n    padding: 0 150px;\r\n  }\r\n\r\n  .home {\r\n    margin-top: 100px;\r\n  }\r\n  .home__image {\r\n    margin: 42px 0;\r\n  }\r\n\r\n  .works {\r\n    display: grid;\r\n    grid-template-columns: 1fr 1fr;\r\n    grid-template-rows: repeat(5, auto);\r\n    column-gap: 30px;\r\n    row-gap: 30px;\r\n\r\n    .work:nth-child(1) {\r\n      grid-column: 1/2;\r\n      grid-row: 2/4;\r\n    }\r\n    .work:nth-child(2) {\r\n      grid-column: 2/2;\r\n      grid-row: 1/3;\r\n    }\r\n    .work:nth-child(3) {\r\n      grid-column: 1/2;\r\n      grid-row: 4/6;\r\n    }\r\n    .work:nth-child(4) {\r\n      grid-column: 2/2;\r\n      grid-row: 3/5;\r\n    }\r\n  }\r\n}\r\n\r\n@media screen and (min-width: 1200px) {\r\n  .container {\r\n    padding: 0 250px;\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"c128b070fcbf35678c79adee0c6ce309","cacheData":{"env":{}}}