n  grid-column: 2/2;\r\n  grid-row: 3/4;\r\n  /* height: 162px;\r\n  width: 167px; */\r\n}\r\n.clients__gallery img:nth-child(3) {\r\n  grid-column: 1/2;\r\n  grid-row: 2/3;\r\n  /* height: 178px;\r\n  width: 178px; */\r\n}\r\n\r\n.clients__quote {\r\n  margin-top: 100px;\r\n  font-weight: 500;\r\n  font-size: 24px;\r\n  line-height: 36px;\r\n}\r\n\r\n.clients__person-box {\r\n  margin-top: 27px;\r\n  display: flex;\r\n}\r\n.clients__person-box img {\r\n  height: 82px;\r\n  width: 82px;\r\n  border-radius: 12px;\r\n}\r\n.details {\r\n  display: flex;\r\n  margin-left: 30px;\r\n  flex-direction: column;\r\n  justify-content: space-between;\r\n}\r\n.details__name {\r\n  font-weight: 500;\r\n  font-size: 24px;\r\n  line-height: 36px;\r\n  color: var(--clr-txt-black);\r\n}\r\n.details__nickname {\r\n  font-weight: 500;\r\n  font-size: 18px;\r\n  line-height: 27px;\r\n  color: #828282;\r\n}\r\n\r\n/* Contact section */\r\n.contact {\r\n  margin-top: 100px;\r\n  background: black;\r\n  padding: 41px 36px;\r\n}\r\n.menu-footer li a {\r\n  color: white;\r\n  text-decoration: none;\r\n  font-size: 18px;\r\n  line-height: 200%;\r\n}\r\n\r\n.logo-footer {\r\n  margin-top: 64px;\r\n  font-family: \"Heebo\", sans-serif;\r\n  color: white;\r\n  font-weight: 800;\r\n  font-size: 36px;\r\n  line-height: 53px;\r\n  color: #f2f2f2;\r\n}\r\n\r\n.social {\r\n  display: flex;\r\n  margin-top: 8px;\r\n}\r\n.social-link {\r\n  margin-right: 12px;\r\n}\r\n.form-footer {\r\n  margin-top: 65px;\r\n}\r\n.copyright {\r\n  font-size: 14px;\r\n  line-height: 17px;\r\n  margin-top: 133px;\r\n  color: #a9a9a9;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"e9b2912a769c2c32045128747071b426","cacheData":{"env":{}}}